{
  "name": "Crictension",
  "version": "1.0.0",
  "description": "A beautiful extension to get live cricket scores, match updates, and notifications for your favorite matches",
  "manifest_version": 3,
  "author": "Poreddy Dinesh Reddy",
  "action": {
    "default_popup": "index.html",
    "default_title": "Crictension - Live Cricket Scores",
    "default_icon": {
      "16": "./logo.jpg",
      "32": "./logo.jpg",
      "48": "./logo.jpg",
      "128": "./logo.jpg"
    }
  },
  "icons": {
    "16": "./logo.jpg",
      "32": "./logo.jpg",
      "48": "./logo.jpg",
      "128": "./logo.jpg"
  },
  "permissions": [
    "notifications",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  }
}